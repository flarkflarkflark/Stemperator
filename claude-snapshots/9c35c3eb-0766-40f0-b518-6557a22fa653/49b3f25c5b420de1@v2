[settings]
tab.global = Algemeen
tab.functional = Controller
tab.display = Weergave
language.label = ReaSonus UI-taal
language.tooltip = Selecteer de taal die je wilt gebruiken voor de UI-elementen in ReaSonus
language.button.label = Talen bewerken
plugin-control.label = Plugin Besturing Uitschakelen
plugin-control.tooltip = Als je een andere controller gebruikt voor het besturen van plugins, kun je dit selecteren. De pluginmodus is nog steeds actief en je kunt nog steeds het pluginvenster openen. Vanaf dat punt zal de ReaSonus FaderPort niets meer doen voor de plugins.
untouch-after-learn.label = Raak laatste aangeraakte parameter los na aanleren
untouch-after-learn.tooltip = Indien geselecteerd, wordt de laatste aangeraakte parameter op niet-aangeraakt gezet na het aanleren van parameters.
master-fader.label = Master fader-modus inschakelen
master-fader.tooltip = Indien geselecteerd en het indrukken van de master-knop schakelt de master fader-modus in. Dit zorgt ervoor dat de laatste track van de FaderPort de master track bestuurt.
swap-shift.label = Shift-knoppen verwisselen
swap-shift.tooltip = Dit wisselt de linker en rechter Shift-knop om.
fader-reset.label = Fader Reset: Linker shift en fader aanraken
fader-reset.tooltip = Indien ingeschakeld, kun je de [Linker Shift] knop gebruiken om de faderwaarde te resetten terwijl je de fader aanraakt.
momentary-mute.label = Mute/Solo Tijdelijk
momentary-mute.tooltip = Indien ingesteld, worden de Solo en Mute knoppen tijdelijk. De timeout voor tijdelijk zijn is 500 milliseconden. Kort gezegd: als je de solo-knop langer dan 500ms indrukt en loslaat, zal deze meteen weer unsolo worden.
control_hidden_track.label = Verborgen tracks besturen
control_hidden_track.tooltip = Sla de verborgen tracks niet over bij het scrollen door de tracks
can_disable_fader.label = Schakel de optie in om faderbeweging uit te schakelen bij het selecteren van een andere track
can_disable_fader.tooltip = Dit schakelt de optie in om de faderbeweging uit te schakelen bij het selecteren van een andere track. Dus geen faderbewegingen tijdens het doen van andere dingen in REAPER.
overwrite-timecode.label = Tijdcode Overschrijven
overwrite-timecode.tooltip = Overschrijft de tijdcode die is geselecteerd in REAPER.
timecode-list.label = Tijdcode
timecode-list.tooltip = Selecteer het type tijdcode waarmee je de tijdcode van REAPER wilt overschrijven.
timecode-list.option.time = Tijd
timecode-list.option.beats = Maatslag
timecode-list.option.seconds = Seconden
timecode-list.option.samples = Samples
timecode-list.option.hr-min-sec-fr = Uu:Min:Sec:Fr
timecode-list.option.frames = Abs. Frames
display-track.label = Track modus weergave
display-track.tooltip = Selecteer de weergavestijl voor de track modus weergave.
display-track.option.display-mode-4 = Groot, Groot (Geen pan info)
display-track.option.display-mode-5 = Klein, Klein, Groot
display-track.option.display-mode-7 = Groot, Klein, Klein
display-track.option.display-mode-8 = Klein, Groot, Klein
distraction-free-mode.label = Afleidingsvrije modus
distraction-free-mode.tooltip = Verwijder alles wat afleidend kan zijn, zoals knipperende knoppen, sommige knoppen in 'aan' staat,
endless_track_scroll.label = Eindeloos scrollen door tracks inschakelen
endless_track_scroll.tooltip = Wanneer je op de laatste track bent en op de volgende knop klikt, ga je naar de eerste track. Hetzelfde voor vorige, maar dan andersom.
latch-preview-action-enable.label = Schakel schrijfactie in voor Latch Preview modus automatisering
latch-preview-action-enable.tooltip = Indien ingeschakeld en de automatiseringsmodus is in Latch Preview, activeert de Latch Preview knop de geselecteerde schrijfactie
track-color-brightness.label = Track kleur helderheid
track-color-brightness.tooltip = Stel de helderheid van de trackkleur in voor de niet-geselecteerde staat. Dit maakt de kleur in de niet-geselecteerde modus meer zichtbaar.
action.save.message = Hoera! Wijzigingen succesvol opgeslagen
popup.unsaved.title = Niet-opgeslagen wijzigingen
popup.unsaved.message = Wil je deze eerst opslaan?

[about]
about.title = Over
about.top = Bedankt voor het gebruiken van ReaSonus Native. ReaSonus Native heeft naast ±95% van de Studio One functies ook:
about.list.1 = 'Onbeperkt' aangepaste bewerkbare filters
about.list.2 = MIDI-leer stijl plugin koppeling
about.list.3 = Shift-knoppen verwisselen
about.list.4 = Laatst aangeraakte plugin parameter besturen
contribute.title = Bijdragen & Links
contribute.top = ReaSonus Native is een gratis en open source project, maar kost wat tijd en moeite om te maken en te onderhouden. Het is natuurlijk niet verplicht, maar een donatie voor onderhoud zou zeer gewaardeerd worden.
contribute.link.coffee = Koop een koffie voor mij
contribute.link.tipeee = Geef mij een fooi op Tipeee
contribute.center = ReaSonus Native heeft een website voor de documentatie. Controleer deze eerst voordat je vragen stelt.
contribute.link.documentation = ReaSonus Native Documentatie
contribute.bottom = Als je een bug of iets anders tegenkomt, rapporteer het alsjeblieft, zodat ReaSonus Native alleen maar beter kan worden
contribute.link.bug = Rapporteer je bug
contribute.link.question = Stel je vraag
thanks.title = Dankwoord
thanks.bottom = Dit project zou niet mogelijk zijn geweest zonder jou, die dit leest. Maar ook een grote dank aan de volgende mensen:
thanks.list.1 = Geoff Waddington, De man achter het indrukwekkende CSI. Zonder het CSI-project zou dit waarschijnlijk nooit zijn gemaakt.
thanks.list.2 = Cfillion, voor het helpen met een aantal ergernissen waar ik tegenaan liep met C++ en CMake.
thanks.list.3 = Alle vrijwilligers die helpen met testen, het controleren van de documentatie en geduldig zijn.
